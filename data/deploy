#!/bin/bash

OUT=data.tgz

tar czvf ${OUT} --exclude='*.blend*' \
    test/workcell_full_0327* \
    test/workcell_boxes_pick* \
    test/workcell_totes_stow* \
    robots/*.* \
    robots/tx90l \
    robots/gripper7 \
    simulation \
    objects/*.* \
    terrains

scp ${OUT} mtd13@login.oit.duke.edu:~/public_html/arc-2017/${OUT}

rm ${OUT}
